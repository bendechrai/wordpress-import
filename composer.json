{
    "name": "bendechrai/wordpress-import",
    "description": "Provides CLI commands to import Wordpress contents from a Wordpress eXtended RSS (WXR) export file.",
    "type": "library",
    "license": "GPLv2",
    "keywords": ["wordpress", "import", "wxr", "laravel"],
    "authors": [
        {
            "name": "Ben Dechrai",
            "email": "ben@dechrai.com"
        }
    ],
    "require": {
        "php": ">=7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0",
        "phpmd/phpmd": "^2.6",
        "codeclimate/php-test-reporter": "^0.4.4",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "BenDechrai\\WordpressImport\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BenDechrai\\WordpressImport\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "BenDechrai\\WordpressImport\\WordpressImportServiceProvider"
            ]
        }
    }
}
